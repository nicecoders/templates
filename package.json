{
  "name": "electron-template",
  "version": "1.0.2",
  "author": {
    "name": "dev"
  },
  "description": "eggKnife-electron的客户端模板",
  "scripts": {
    "dev": "cross-env umi dev",
    "build": "cross-env NODE_ENV=production umi build",
    "postinstall": "cross-env umi setup",
    "setup": "cross-env umi setup",
    "start": "cross-env npm run dev",
    "test": "cross-env TS_NODE_TRANSPILE_ONLY=yes jest --passWithNoTests"
  },
  "build": {
    "productName": "测试",
    "appId": "nicecode"
  },
  "dependencies": {
    "antd": "^5.16.1",
    "chokidar": "^3.6.0",
    "clear-module": "^4.1.2",
    "lodash": "^4.17.21",
    "umi": "^4.0.42"
  },
  "devDependencies": {
    "@tsconfig/node21": "^21.0.3",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/plugin-electron": "^0.2.0",
    "electron": "29.1.6",
    "typescript": "^5",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "jest": "^29",
    "@types/jest": "^29",
    "ts-node": "^10",
    "cross-env": "^7",
    "jest-environment-jsdom": "^29",
    "@testing-library/jest-dom": "^5",
    "@testing-library/react": "^14",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@umijs/plugins": "^4.1.8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "src/**/*",
    "tsconfig.json",
    ".npmrc",
    ".gitignore",
    "README.md",
    ".umirc.ts",
    "package.json",
    "typings.d.ts"
  ]
}